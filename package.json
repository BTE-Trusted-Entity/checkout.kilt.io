{
  "repository": "git@github.com:BTE-Trusted-Entity/checkout.kilt.io.git",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">= v18.16.1"
  },
  "scripts": {
    "prettify": "prettier --write \"src/**/*.ts*\" \"src/**/*.css*\"",
    "lint": "yarn run lint-css --formatter github && yarn run lint-js",
    "lint-js": "eslint --max-warnings 0 --config ./.eslintrc.json \"src/**/*.ts*\"",
    "lint-css": "stylelint --max-warnings 0 --fix \"src/**/*.css\"",
    "css": "tcm --namedExports --pattern 'src/frontend/**/*.module.css' && eslint --fix --config ./.eslintrc.json \"src/**/*.css.d.ts\"",
    "test": "tsc --noEmit && jest",
    "dev": "yarn parcel watch",
    "build": "rm -rf dist && NODE_OPTIONS=--no-experimental-fetch yarn parcel build",
    "start": "node --enable-source-maps dist/backend/server.js",
    "dev-start": "echo '{\"type\":\"commonjs\"}' > dist/package.json && supervisor --watch dist/backend/ --enable-source-maps dist/backend/server.js",
    "storybook": "storybook dev --port 6007"
  },
  "dependencies": {
    "@hapi/basic": "^7.0.2",
    "@hapi/boom": "^10.0.1",
    "@hapi/hapi": "^21.3.2",
    "@hapi/inert": "^7.1.0",
    "@kiltprotocol/sdk-js": "^0.33.1",
    "@paypal/react-paypal-js": "^8.1.0",
    "@polkadot/ui-shared": "^3.5.1",
    "aws-sdk": "^2.1414.0",
    "dotenv": "^16.3.1",
    "exiting": "^6.1.0",
    "got": "^11.8.6",
    "hapi-dev-errors": "^4.0.0",
    "hapi-pino": "^12.1.0",
    "ky": "^0.33.3",
    "nodemailer": "^6.9.3",
    "pino": "^8.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.6.1",
    "@paypal/paypal-js": "^6.0.0",
    "@storybook/addon-actions": "^7.0.27",
    "@storybook/addon-controls": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/addon-toolbars": "^7.0.27",
    "@storybook/addon-viewport": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-webpack5": "^7.0.27",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/hapi__basic": "^5.1.3",
    "@types/hapi__hapi": "^20.0.13",
    "@types/hapi__inert": "^5.2.6",
    "@types/jest": "^29.5.3",
    "@types/nodemailer": "^6.4.8",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "hapi-dev-errors": "^4.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "parcel": "^2.7.0",
    "pino-pretty": "^10.0.1",
    "prettier": "^3.0.0",
    "storybook": "^7.0.27",
    "stylelint": "^15.10.1",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-standard": "^34.0.0",
    "supervisor": "^0.12.0",
    "ts-jest": "^29.1.1",
    "typed-css-modules": "^0.7.2",
    "typescript": "^5.1.6"
  },
  "targets": {
    "frontend": {
      "context": "browser",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      },
      "source": [
        "src/frontend/index.html",
        "src/frontend/terms.html",
        "src/frontend/privacy.html",
        "src/frontend/imprint.html"
      ],
      "distDir": "dist/frontend"
    },
    "backend": {
      "context": "node",
      "source": "src/backend/server.ts",
      "distDir": "dist/backend",
      "outputFormat": "esmodule"
    }
  }
}
